{
    "version": 2,
    "functions": {
      "api/*.js": {
        "includeFiles": "src/config/**"
      }
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }